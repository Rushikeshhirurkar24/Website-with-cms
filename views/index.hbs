  {{!-- contact us section --}}

  <section id="contact_us_section" class="py-5 background1">

   <div class="container">
      <h3 class="text-center text-light"> contact us</h3>

      <div class="row">
         <div class="col-md-12">

            <form action="/process-contact-form" method="post">
                   <div class="mb-3">
                     <label for="exampleInputEmail1" class="form-label text-light">your email address</label>
                     <input name="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                     <div id="emailHelp" class="form-text text-light">We'll never share your email with anyone else.</div>
                   </div>
                   <div class="mb-3">
                     <label for="contactNumberField" class="form-label text-light">your contact number</label>
                     <input  name="phone" type="number" class="form-control" id="exampleInputPassword1">
                   </div>
                
                    <div class="form-floating mt-2 py-5">
                    <textarea name="query" class="form-control" placeholder="Enter your query here" id="QueryField" style="height: 250px"></textarea>
                    <label  for="QueryField" class="text-light">Your Query :-</label>
                     </div>

                <div class="container text-center" >
                   <button type="submit" class="btn btn-light text-solid">Submit</button>
                   </div>
                 </form>
         </div>
